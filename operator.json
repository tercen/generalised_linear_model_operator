{
  "name": "Logistic Regression",
  "description": "Fits a logistic regression model to your data.",
  "tags": [
    "statistical testing",
    "linear model"
  ],
  "authors": [
    "tercen"
  ],
  "urls": [
    "https://github.com/tercen/generalised_linear_model_operator"
  ],
  "container": "tercen/generalised_linear_model_operator:0.1.5",
  "properties": [
    {
      "kind": "DoubleProperty",
      "name": "N",
      "defaultValue": 1,
      "description": "Size N (binomial distribution parameter)."
    },
    {
      "kind": "EnumeratedProperty",
      "name": "model.family",
      "defaultValue": "binomial",
      "values": [
        "binomial",
        "negative.binomial",
        "gaussian"
      ],
      "description": "Model family (any of binomial, negative binomial, gaussian)."
    }
  ],
  "operatorSpec": {
    "kind": "OperatorSpec",
    "ontologyUri": "https://tercen.com/_ontology/tercen",
    "ontologyVersion": "0.0.1",
    "inputSpecs": [
      {
        "kind": "CrosstabSpec",
        "metaFactors": [
          {
            "kind": "MetaFactor",
            "name": "Fixed Effects",
            "type": "",
            "description": "Fixed effects (numeric or character)",
            "ontologyMapping": "variable",
            "crosstabMapping": "column",
            "cardinality": "1..n",
            "factors": []
          }
        ],
        "axis": [
          {
            "kind": "AxisSpec",
            "metaFactors": [
              {
                "kind": "MetaFactor",
                "name": "Response Variable",
                "type": "numeric",
                "description": "Response variable (must be a count)",
                "ontologyMapping": "measurement",
                "crosstabMapping": "y",
                "cardinality": "1",
                "factors": []
              },
              {
                "kind": "MetaFactor",
                "name": "Random Effects - Slopes",
                "type": "",
                "description": "Random effects on slopes (numeric or character)",
                "ontologyMapping": "factor",
                "crosstabMapping": "color",
                "cardinality": "0..n",
                "factors": []
              },
              {
                "kind": "MetaFactor",
                "name": "Random Effects - Intercept",
                "type": "",
                "description": "Random effects on intercept (numeric or character)",
                "ontologyMapping": "factor",
                "crosstabMapping": "label",
                "cardinality": "0..n",
                "factors": []
              }
            ]
          }
        ]
      }
    ]
  }
}